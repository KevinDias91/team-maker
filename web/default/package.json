    "babel-preset-react": "^6.1.2",
    "browser-sync": "^2.23.5",
    "concurrently": "^3.5.1",
    "cssnano": "^3.10.0",
    "cssnext": "^1.8.4",
    "del": "^2.2.2",
    "express": "^4.16.2",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.2.0",
